{
  "name": "esigi-collaborator",
  "version": "0.0.0",
  "scripts": {
    "install": "concurrently --kill-others-on-fail \"npm run install:collaborator\" \"npm run install:jobs\"  \"npm run install:resume\"  \"npm run install:webapp\"",
    "install:collaborator": "cd ms-collaborator && npm install",
    "install:jobs": "cd ms-jobs && npm install",
    "install:resume": "cd ms-resume && npm install",
    "install:webapp": "cd webapp && npm install",
    "start": "concurrently --kill-others-on-fail \"npm run start:collaborator\" \"npm run start:jobs\"  \"npm run start:resume\"  \"npm run start:webapp\"",
    "start:collaborator": "cd ms-collaborator && npm run start:dev",
    "start:jobs": "cd ms-jobs && npm run start:dev",
    "start:resume": "cd ms-resume && npm run start:dev",
    "start:webapp": "cd webapp && npm run start"
  },
  "private": true,
  "dependencies": {
    "@nestjs/typeorm": "^8.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "mysql2": "^2.3.3",
    "ngx-mask": "^13.1.1",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
